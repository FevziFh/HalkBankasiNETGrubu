@model IList<Student>
<div style="border:2px solid red;">
    <div>
        <h2>Merhaba Dünya</h2>
    </div>
    <div>
        <p style="font-weight:bold;">
            Bugün: @DateTime.Now.ToString()
        </p>
    </div>
    <div>
        <table class="table">
            <tr>
                <th>
                    ıd
                </th>
                <th>
                    Name
                </th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                </tr>
            }
        </table>
    </div>
</div>