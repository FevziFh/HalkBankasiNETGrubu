@model IList<Product>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
@await Html.PartialAsync("_Greeting")
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Title</th>
            <th scope="col">Price</th>
            <th scope="col">Detail</th>
        </tr>
    </thead>
    <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <th scope="row">@item.Id</th>
                    <th>@item.Title</th>
                    <th>@item.Price</th>
                    <th><a asp-controller="Product" asp-action="Detail" asp-route-Id="@item.Id">Detay</a></th>
                </tr>
            }
    </tbody>
</table>
